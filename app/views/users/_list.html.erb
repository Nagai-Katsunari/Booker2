<span id="title">
  <h1>Users</h1>
</span>
<table class="table">
  <thead>
    <tr>
     <th>Image</th>
      <th>name</th>

    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= attachment_image_tag user, :profile_image, format: 'jpeg', fallback: "no_image.jpg", size: "100x100" %></td>
      <td><%= user.name%></td>
      <td> <%= link_to "Show", user_path(user) %> </td>
     
    </tr>
    <% end %>
  </tbody>
</table>